    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "remotePath": "",
            "port": 2345,
            "host": "127.0.0.1",
            "showGlobalVariables": true,
            "program": "${workspaceRoot}",
            "buildFlags": "-gcflags='all=-N -l'",
            "showLog": true
        },
        {
            "name": "Execute",
            "type": "go",
            "request": "launch",
            "mode": "exec",
            "remotePath": "",
            "port": 2345,
            "host": "127.0.0.1",
            "showGlobalVariables": true,
            "program": "bin/githubinfo",
            "buildFlags": "-gcflags='all=-N -l'",
            "showLog": true 
        }
    ]
}